<?xml version="1.0" encoding="Windows-1251"?>
<cliche>
	<formulas>
		<formula name="calcDate" formula="calcdate(1, 'day', -2, 'year', -2, 'week', -2, 'month', 'yyyy-MM-dd')"/>
		<formula name="currDate" formula="currentdate('yyyy-MM-dd')"/>
		<formula name="currDate1" formula="currentdate('yyyyMMdd')"/>
		<formula name="currDateTime" formula="currentdate('yyyy-MM-dd hh:mm:ss')"/>
		<formula name="inc1" formula="inc(0,1)" isLoop="true"/>
		<formula name="inn" formula="inn('fl','7812')" isLoop="true"/>
		<formula name="ogrn" formula="ogrn()" isLoop="true"/>
		<formula name="ogrnip" formula="ogrnip()" isLoop="true"/>
		<formula name="inn1" formula="inn('ul','78')" isLoop="true"/>
		<formula name="inn2" formula="inn('inul')" isLoop="true"/>
		<formula name="inn3" formula="inn('fl')" isLoop="true"/>
		<formula name="schet" formula="rschet()" isLoop="true"/>
		<formula name="schet1" formula="rschet('810','103','22','33',38)" isLoop="true"/>
		<formula name="kpp" formula="kpp('fl','7812')" isLoop="true"/>
		<formula name="randomset" formula="randomset('fl','7812')" isLoop="true"/>
		<formula name="incpadleft" formula="incpadleft(0,1,6,'0')"/>
		<formula name="incpadleft1" formula="incpadleft(0,1,6,'0')" isLoop="true"/>
		<formula name="faker" formula="faker(date.birthday(18;65), 'yyyy-MM-dd')" isLoop="true"/>
		<formula name="fakerDune" formula="faker(dune.character)" isLoop="true"/>
		<formula name="fakerPhone" formula="faker(phoneNumber.phoneNumber)" isLoop="true"/>
	</formulas>
	<loopTexts>
		<loopText name="text" amountCopyes="20">
<![CDATA[
				<loopText>
					<element calcDate="${currDate}"/>
					<element currDate="${currDate}"/>
					<element currDate1="${currDate1}"/>
					<element currDateTime="${currDateTime}"/>
					<element inc1="${inc1}"/>
					<element inn="${inn}"/>
					<element inn2="${inn2}"/>
					<element inn3="${inn3}"/>	
					<element schet="${schet}"/>	
					<element schet1="${schet1}"/>	
					<element kpp="${kpp}"/>	
					<element randomset="${randomset}"/>	
					<element incpadleft="${incpadleft}"/>	
					<element incpadleft1="${incpadleft1}"/>	
					<element faker="${faker}"/>	
					<element fakerDune="${fakerDune}"/>	
					<element fakerPhone="${fakerPhone}"/>	
				</loopText> ]]></loopText>
		<loopText name="loopText" amountCopyes="10"><![CDATA[ some kind of repetitive text]]></loopText>
	</loopTexts>
	<fileNameMask>TestDocument_${currDate1}_${incpadleft}.xml</fileNameMask>
	<amountCopyes>20</amountCopyes>
	<toSubFolder>false</toSubFolder>
	<encoding>windows-1251</encoding>
	<xmlVersion>1.0</xmlVersion>
	<mainText>
<![CDATA[
<root dateFile="${currDate}">
	<element calcDate="${currDate}"/>
	<element currDate="${currDate}"/>
	<element currDate1="${currDate1}"/>
	<element currDateTime="${currDateTime}"/>
	<element inc1="${inc1}"/>
	<element inn="${inn}"/>
	<element inn2="${inn2}"/>
	<element inn3="${inn3}"/>	
	<element schet="${schet}"/>	
	<element schet1="${schet1}"/>	
	<element kpp="${kpp}"/>	
	<element randomset="${randomset}"/>	
	<element incpadleft="${incpadleft}"/>	
	<element incpadleft1="${incpadleft1}"/>	
	<element faker="${faker}"/>	
	<element fakerDune="${fakerDune}"/>	
	<element fakerPhone="${fakerPhone}"/>	
	<element loopText="${loopText}"/>	
	${text}
</root>
		]]></mainText>
</cliche>